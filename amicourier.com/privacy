 <script>
function refreshThisPage(){
	//window.location.reload();
	location.href = "";
	}
	 
function subscribe(){
var hr = new XMLHttpRequest();
var  email = document.getElementById('submail').value;
var  name = document.getElementById('subname').value;
var  subscribeme = "subscribeme"
var url = "reg_process.php";
var vars = "submail="+email+"&subname="+name+"&subscribeme="+subscribeme;
//var vars = "log="+device;
if(email=="" || name==""){
	//swal.fire("Please enter the email attached to your wallet!");
	swal.fire({
					type:'warning',
					title:'Field Empty',
					html:'Please enter required details'
					
				});
	}else{
	
var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
		if( !emailReg.test(email) ) {
					//swal.fire('Please use a valid email address!');
			swal.fire({
					type:'warning',
					title:'Invalid Email',
					html:'Please use a valid email address not ' + email
					
				});
		}else{
			
			
			$('i#sp5').attr("class","fa fa-spinner fa-spin");
			
			//$('#sub').click();
			hr.open("POST", url, true);
	hr.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
	// Access the onreadystatechange event for the XMLHttpRequest object
	hr.onreadystatechange = function() {
	  //  console.log(hr);
		if(hr.readyState == 4 && hr.status == 200) {
			var return_data = hr.responseText;
		//document.getElementById('vali_mail').innerHTML = return_data;
		//swal.fire(return_data);
			if(return_data==100 || return_data==true){
				
			 swal.fire({
					type:'success',
					title:'Done',
					html:'Thank you for subscribing ' + email
					
				});
				
			}else if(return_data==300){
			
				 swal.fire({
					type:'success',
					title:'Done',
					html:'Thank you for activating News Update for ' + email
					
				});
				
				
			}else if(return_data==false){
			 swal.fire({
					type:'error',
					title:'Failed',
					html:'There was an error subscribing ' + email
					
				});	
				
				
			}else{
				
			swal.fire({
					type:'error',
					title:'Please Retry',
					html:return_data
					
				});	
				
			}
			
		$('i#sp5').attr("class","fa fa-send-o");
			
			//setTimeout(refreshPage,3500);
			setTimeout(refreshThisPage,3500)
		}
	}
	hr.send(vars); // Actually execute the request
			
	$('i#sp5').attr("class","la la-send-o");
			swal.fire('sending...');
							
 
	
           }//email
		
		
		
		
      	
		
	
	}//else empty
//swal.fire('sending...');

}
	
$(function(){
 
	$('#submail').keyup(function()
	{
		var yourrInput = $(this).val();
		
			//var no_spl_char = yourrInput.replace(/[`~!@#$%^&*()_|+\-=?;:'",.<>\{\}\[\]\\\/]/gi, '');
			var rem_space = yourrInput.replace(/ /g,'');
			//$(this).val(no_spl_char);
			$(this).val(rem_space);
		
		
		
		
		
	});
	
 
});	
	 
	 
	 
 </script>
