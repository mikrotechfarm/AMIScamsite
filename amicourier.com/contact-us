       <script>
function contact_me(){
var hr = new XMLHttpRequest();
var  name = document.getElementById('conname').value;
var  phone = document.getElementById('conphone').value;
var  email = document.getElementById('conemail').value;
var  subject = document.getElementById('consubject').value;
var  message = document.getElementById('conmessage').value;

var contactmail = "contactmail";
var url = "reg_process.php";
var vars = "conname="+name+"&conemail="+email+"&conphone="+phone+"&consubject="+subject+"&conmessage="+message+"&contactmail="+contactmail;

if(email =="" || name =="" || message ==""){
	//swal.fire("Please enter the email attached to your wallet!");
	swal.fire({
					type:'warning',
					title:'Field Empty',
					html:'Please enter required details'
					
				});
	}
	else{
var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
		if( !emailReg.test(email) ) {
					//swal.fire('Please use a valid email address!');
			swal.fire({
					type:'warning',
					title:'Invalid Email',
					html:'Please use a valid email address not ' + email
					
				});
		}else{
        $('i#sp5').attr("class","fa fa-spinner fa-spin");
hr.open("POST", url, true);
	hr.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
	// Access the onreadystatechange event for the XMLHttpRequest object
	hr.onreadystatechange = function() {
	  //  console.log(hr);
		if(hr.readyState == 4 && hr.status == 200) {
			var return_data = hr.responseText;
			//swal.fire(return_data);
			if(return_data==100 || return_data==true){
				
				swal.fire({
					type:'success',
					title:'Mail Sent',
					html:'Your mail has been sent, we will attend to you shortly '
					
				});
				
			}else{
			if(return_data==false){
				
				swal.fire({
					type:'error',
					title:'Failed',
					html:'Sorry we could not send mail to' + email
					
				});
				
			}else{
			swal.fire({
					type:'warning',
					title:'Warning Info',
					html: return_data
					
				});	
				
			}
				
				
			}
				$('i#sp5').attr("class","fa fa-send-o");
			
			//setTimeout(refreshPage,2000);
		}
	}
	hr.send(vars); // Actually execute the request
			
		swal.fire('sending Message...');
		
           }//email
      }//else empty
//swal.fire('processing...');



}
	
	
</script>
	</body>
